package objectclass;


class Test{}

class Example
{
	int x=10;
	int y=20;
}

public class Comparision
{
	
	public static void main(String[] args) {
		
		
		int x=10;
		int y=20;
		int z=10;
		
		boolean bo=true;
		
		System.out.println(x==y);
		
		System.out.println(y==2);
		
		
		Example e1=new Example();
		Example e2=new Example();
		
		Example e3=null;
		
		Test t1=new Test();
		
